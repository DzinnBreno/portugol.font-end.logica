programa {
  funcao inicio() {
    inteiro num1, num2

    escreva("Digite o primeiro número: \n")
    leia(num1)
    escreva ("Digite o número: \n")
    leia(num2)

    se(num1 > num2){
      escreva("O maior número é: ", num1)
    }
    senao{
      escreva("O Maior número é: ", num2)
    }
  }
}
