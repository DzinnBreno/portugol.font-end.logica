programa {
  funcao inicio() {
    real num1, num2, num3, media
    
    escreva("Dgite o primeiro número: \n")
    leia(num1)
    escreva("Digite o segundo número: \n")
    leia(num2)
    escreva("Digite o teceiro número: \n")
    leia(num3)

    media = (num1+num2+num3) / 3
    escreva("A média aritmética é: ", media)
  }
}
